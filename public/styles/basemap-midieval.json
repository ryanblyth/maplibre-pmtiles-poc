      "minzoom": 0,
      "maxzoom": 6,
      "filter": ["==", ["get", "admin_level"], 4],
      "paint": {
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          0, 1.5, 6, 3
        ],
        "line-color": "hsl(26, 27%, 59%)",
        "line-dasharray": [3, 1]
